@model IDictionary<string, int>

@{
    ViewBag.Title = "EquipmentCountByCategory";
    Layout = null;
}


    <div class="card mb-4">
        <div class="card-header">
            <i class="fas fa-table me-1"></i>
            Category List
        </div>
        <div class="card-body">

            <table class="table">
                <thead>
                    <tr>
                        <th>Category</th>
                        <th>Total Count</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Key</td>
                            <td>@item.Value</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
